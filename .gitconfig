[user]
	name = Emily Ye
	email = emily.ye@shopify.com
[alias]
	st = status
	co = checkout
	ci = commit
	ca = commit --amend -C HEAD
	br = branch --sort=-committerdate
	df = diff
	dfs = diff --staged
	lg = log --oneline --decorate --graph --abbrev-commit
        fp = push --force-with-lease
[push]
	default = simple
[pull]
	rebase = true
[color]
	ui = true
[sequence]
	editor = rebase-editor
[fetch]
	prune = true
